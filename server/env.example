# Server Configuration
PORT=3001
NODE_ENV=development

# Frontend URL (for CORS)
FRONTEND_URL=http://localhost:5173

# Airbnb Calendar URLs
# Single URL (add your Airbnb iCal URL here)
AIRBNB_CALENDAR_URL=

# Multiple URLs (JSON array) - add your Airbnb iCal URLs here
AIRBNB_CALENDAR_URLS=[]

# Per-room calendar URLs (add your room-specific Airbnb iCal URLs here)
AIRBNB_CALENDAR_URL_ROOM_1=
AIRBNB_CALENDAR_URL_ROOM_2=
AIRBNB_CALENDAR_URL_ROOM_3=

# API Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Demo Data Control (set to 'true' only for testing)
USE_DEMO_DATA=false 